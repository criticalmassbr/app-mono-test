{
    "name": "frontend-nextjs",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "prisma:seed": "tsc ./prisma/seed/names.ts && node ./prisma/seed/names.js",
        "clear": "yarn workspace frontend-nextjs cache clear && rm -rf ./.next && rm -rf ./node_modules && yarn workspace frontend-nextjs install",
        "migrate:dev": "yarn workspace frontend-nextjs dlx prisma migrate dev --schema=./prisma/schema.prisma"
    },
    "dependencies": {
        "@emotion/react": "^11.13.0",
        "@emotion/styled": "^11.13.0",
        "@mui/icons-material": "^5.16.7",
        "@mui/material": "^5.16.7",
        "@prisma/client": "^5.18.0",
        "bcrypt": "^5.1.1",
        "jose": "^5.6.3",
        "jsonwebtoken": "^9.0.2",
        "next": "14.2.5",
        "react": "^18",
        "react-dom": "^18"
    },
    "devDependencies": {
        "@faker-js/faker": "^8.4.1",
        "@types/bcrypt": "^5",
        "@types/jsonwebtoken": "^9",
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "eslint": "^8",
        "eslint-config-next": "14.2.5",
        "postcss": "^8",
        "prisma": "^5.18.0",
        "tailwindcss": "^3.4.1",
        "ts-node": "^10.9.2",
        "typescript": "^5"
    },
    "engines": {
        "node": ">=20.10.0"
    },
    "packageManager": "yarn@4.4.0"
}
